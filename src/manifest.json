{
    "name": "Skype (Linux) Images",
    "description": "Исправляет URL картинок при просмотре их в браузере по ссылке из Linux-версии Skype",
    "version": "1.3.0",
    "manifest_version": 2,
    "icons": {
        "256": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "https://api.asm.skype.com/*",
        "https://login.skype.com/*",
        "webRequest",
        "webRequestBlocking"
    ]
}
